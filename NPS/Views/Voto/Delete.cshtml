@model NPS.Models.Voto

@{
    ViewBag.Title = "Delete";
}
<br />
<br />
<h2>Deletar Voto</h2>

<h3>Tem certeza de que deseja excluir?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">

        <dt>Nota</dt>
        <dd>@Html.DisplayFor(model => model.Vl_voto)</dd>

        <dt>Telefone</dt>
        <dd>@Html.DisplayFor(model => model.Nr_telefone)</dd>

        <dt>Justificativa</dt>
        <dd>@Html.DisplayFor(model => model.Justificativa_voto)</dd>

        <dt>Setor</dt>
        <dd>@Html.DisplayFor(model => model.Setor.Nome)</dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Deletar" class="btn btn-danger" />
            @Html.ActionLink("Voltar", "Index", null, new { @class = "btn btn-primary" })
        </div>
    }
</div>
